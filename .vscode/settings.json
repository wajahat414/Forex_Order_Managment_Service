{
    "C_Cpp.default.cppStandard": "c++20",
    "C_Cpp.default.compilerPath": "/opt/homebrew/opt/llvm/bin/clang++",
    "C_Cpp.default.compilerArgs": [
        "-std=c++20",
        "-fmodules-ts"
    ],
    "C_Cpp.default.compileCommands": "${workspaceFolder}/compile_commands.json",
    "C_Cpp.intelliSenseEngine": "default",
    "C_Cpp.experimentalFeatures": "enabled",
    "cmake.configurePreset": "ninja-llvm",
    "cmake.buildPreset": "ninja-llvm",
    "files.associations": {
        "__hash_table": "cpp",
        "__node_handle": "cpp",
        "__verbose_abort": "cpp",
        "cmath": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "memory": "cpp",
        "initializer_list": "cpp",
        "iosfwd": "cpp",
        "limits": "cpp",
        "new": "cpp",
        "stdexcept": "cpp",
        "string": "cpp",
        "string_view": "cpp",
        "typeinfo": "cpp",
        "unordered_map": "cpp",
        "cstddef": "cpp",
        "algorithm": "cpp",
        "iostream": "cpp",
        "chrono": "cpp",
        "tuple": "cpp",
        "condition_variable": "cpp",
        "sstream": "cpp",
        "regex": "cpp",
        "vector": "cpp",
        "future": "cpp",
        "istream": "cpp",
        "locale": "cpp",
        "__bit_reference": "cpp",
        "ixx": "cpp"
    }
